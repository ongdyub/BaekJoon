    "n = int(input())\n",
    "\n",
    "num_list = list(map(int, input().split()))\n",
    "\n",
    "count = [1] * n\n",
    "\n",
    "for i in range(1,n):\n",
    "    for j in range(i):\n",
    "        if(num_list[j] < num_list[i]):\n",
    "            count[i] = max(count[i], count[j]+1)\n",
    "            \n",
    "print(max(count))\n"
